{
  "name": "@fapalz/scss-starter",
  "version": "1.1.4",
  "description": "Simple scss starter",
  "private": true,
  "scripts": {
    "build": "sass scss/main.scss css/main.css --no-source-map"
  },
  "style": "css/main.css",
  "sass": "scss/main.scss",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fapalz/scss-starter.git"
  },
  "keywords": [
    "scss",
    "css",
    "starter",
    "web",
    "libary"
  ],
  "author": "Gladikov Kirill - Fapalz <blacsesmot@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Fapalz/scss-starter/issues"
  },
  "homepage": "https://github.com/Fapalz/scss-starter#readme",
  "lint-staged": {
    "*.{scss, css}": "stylelint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "husky": "^5.0.9",
    "lint-staged": "^10.5.4",
    "prettier": "2.2.1",
    "sass": "^1.32.6",
    "stylelint": "^13.10.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.19.0"
  }
}
